[calendars]

	[[calendar_default]]
		path = ~/.local/share/calendar/default/
		type = calendar

	[[calendar_birthdays]]
		path = ~/.local/share/contacts/default/
		type = birthdays

[default]
	default_calendar = calendar_default

[locale]
	timeformat = %H:%M
	dateformat = %Y-%m-%d
	longdateformat = %Y-%m-%d
	datetimeformat = %Y-%m-%d %H:%M
	longdatetimeformat = %Y-%m-%d %H:%M
	unicode_symbols = False
